{
  "rewrites": [
    { "source": "/google(.*)\\.html", "destination": "/client/Pages/google$1.html" },
    { "source": "/api/(.*)", "destination": "/api/index.js" },
    { "source": "/health", "destination": "/api/index.js" },
    { "source": "/sitemap.xml", "destination": "/api/index.js" },
    { "source": "/Style/(.*)", "destination": "/client/Style/$1" },
    { "source": "/script/(.*)", "destination": "/client/script/$1" },
    { "source": "/assets/(.*)", "destination": "/client/assets/$1" },
    { "source": "/manifest.json", "destination": "/client/manifest.json" },
    { "source": "/sw.js", "destination": "/client/sw.js" },
    { "source": "/", "destination": "/client/Pages/index.html" },
    { "source": "/index.html", "destination": "/client/Pages/index.html" },
    { "source": "/movies.html", "destination": "/client/Pages/movies.html" },
    { "source": "/series.html", "destination": "/client/Pages/series.html" },
    { "source": "/p_search.html", "destination": "/client/Pages/p_search.html" },
    { "source": "/contact.html", "destination": "/client/Pages/contact.html" },
    { "source": "/auth.html", "destination": "/client/Pages/auth.html" },
    { "source": "/offline.html", "destination": "/client/Pages/offline.html" },
    { "source": "/movies", "destination": "/client/Pages/movies.html" },
    { "source": "/series", "destination": "/client/Pages/series.html" },
    { "source": "/search", "destination": "/client/Pages/p_search.html" },
    { "source": "/contact", "destination": "/client/Pages/contact.html" },
    { "source": "/auth", "destination": "/client/Pages/auth.html" },
    { "source": "/details", "destination": "/client/Pages/details.html" },
    { "source": "/details.html", "destination": "/client/Pages/details.html" },
    { "source": "/offline", "destination": "/client/Pages/offline.html" },
    { "source": "/offline.html", "destination": "/client/Pages/offline.html" },
    { "source": "/movie/:slug", "destination": "/client/Pages/details.html" },
    { "source": "/tv/:slug", "destination": "/client/Pages/details.html" }
  ]
}
