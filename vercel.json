{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index.js" },
    { "source": "/health", "destination": "/api/index.js" },
    { "source": "/sitemap.xml", "destination": "/api/index.js" },
    { "source": "/Style/(.*)", "destination": "/client/Style/$1" },
    { "source": "/script/(.*)", "destination": "/client/script/$1" },
    { "source": "/assets/(.*)", "destination": "/client/assets/$1" },
    { "source": "/manifest.json", "destination": "/client/manifest.json" },
    { "source": "/sw.js", "destination": "/client/sw.js" },
    { "source": "/", "destination": "/client/Pages/index.html" },
    { "source": "/details", "destination": "/client/Pages/details.html" },
    { "source": "/movie/:slug", "destination": "/client/Pages/details.html" },
    { "source": "/tv/:slug", "destination": "/client/Pages/details.html" }
  ]
}
